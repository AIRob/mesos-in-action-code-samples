{
    "id": "docker-nginx",
    "instances": 1,
    "cpus": 0.5,
    "mem": 64.0,
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "nginx:1.9",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 0
                }
            ]
        }
    }
}
